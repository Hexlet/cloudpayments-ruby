module Cloudpayments
  module Resources
    class Users
      def create: (
        ?id: Integer,
        ?email: String,
        ?first_name: String,
        ?last_name: String,
        ?password: String,
        ?phone: String,
        ?username: String,
        ?user_status: Integer,
        ?request_options: Cloudpayments::request_opts
      ) -> Cloudpayments::User

      def retrieve: (
        String username,
        ?request_options: Cloudpayments::request_opts
      ) -> Cloudpayments::User

      def update: (
        String existing_username,
        ?id: Integer,
        ?email: String,
        ?first_name: String,
        ?last_name: String,
        ?password: String,
        ?phone: String,
        ?username: String,
        ?user_status: Integer,
        ?request_options: Cloudpayments::request_opts
      ) -> nil

      def delete: (
        String username,
        ?request_options: Cloudpayments::request_opts
      ) -> nil

      def create_with_list: (
        ?items: ::Array[Cloudpayments::User],
        ?request_options: Cloudpayments::request_opts
      ) -> Cloudpayments::User

      def login: (
        ?password: String,
        ?username: String,
        ?request_options: Cloudpayments::request_opts
      ) -> String

      def logout: (?request_options: Cloudpayments::request_opts) -> nil

      def initialize: (client: Cloudpayments::Client) -> void
    end
  end
end
